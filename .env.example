# GCP Configuration
# Copy this file to .env and update with your actual values

# GCP Project ID
GCP_PROJECT_ID=data-pipeline-platform-484814

# BigQuery Configuration
BQ_DATASET_RAW=mdp_raw
BQ_DATASET_STAGING=mdp_staging
BQ_DATASET_INTERMEDIATE=mdp_intermediate
BQ_DATASET_MARTS=mdp_marts
BQ_LOCATION=europe-west1

# Path to GCP Service Account JSON key
# Set this to use BigQuery
GOOGLE_APPLICATION_CREDENTIALS=/path/to/gcp-key.json